# These function names are also known to
# (and are the plan for transitioning to) run.go.

compile() {
	$G $D/$F.go
}

build() {
	$G $D/$F.go && $L $F.$A
}

run() {
	$G $D/$F.go && $L $F.$A && ./$A.out "$@"
}

errorcheck() {
	errchk $G -e $D/$F.go
}
